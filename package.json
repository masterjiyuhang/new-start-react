		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.3",
		"less": "^4.1.3",
		"lint-staged": "^13.2.2",
		"mockjs": "^1.1.0",
		"nodemon": "^2.0.22",
		"postcss": "^8.4.24",
		"prettier": "^2.8.8",
		"qs": "^6.11.2",
		"rollup-plugin-visualizer": "^5.9.2",
		"sass": "^1.63.6",
		"standard-version": "^9.5.0",
		"stylelint": "^15.9.0",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-recess-order": "^4.2.0",
