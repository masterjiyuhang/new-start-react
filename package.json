	"scripts": {
		"dev": "vite",
		"serve": "vite",
		"server": "nodemon -e ts,tsx --exec ts-node-esm server/index",
		"build": "tsc && vite build",
		"build-only": "vite build",
		"preview": "vite preview",
		"lint": "eslint src",
		"lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
