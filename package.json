{
	"name": "new-start-react",
	"private": true,
	"version": "0.0.1",
	"scripts": {
		"dev": "vite",
		"serve": "vite",
		"server": "nodemon -e ts,tsx --exec ts-node-esm server/index",
		"build": "tsc && vite build",
		"build-only": "vite build",
		"preview": "vite preview",
		"lint": "pnpm run lint:eslint && pnpm run lint:stylelint",
		"lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
		"lint:stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
		"lint-staged": "lint-staged",
		"prepare": "husky install",
		"commit": "git pull && git add -A && git-cz && git push",
		"json-server": "json-server --watch ./server/db.json --port 3001"
	},
	"dependencies": {
		"@ant-design/icons": "^5.3.6",
		"@reduxjs/toolkit": "^1.9.7",
		"@tweenjs/tween.js": "^21.1.1",
		"antd": "^5.16.0",
		"axios": "^1.6.8",
		"clsx": "^2.1.0",
		"delaunator": "^5.0.1",
		"echarts": "^5.5.0",
		"express": "^4.19.2",
		"i18next": "^23.10.1",
		"immer": "^9.0.21",
		"intro.js": "^7.2.0",
		"js-md5": "^0.7.3",
		"lodash-es": "^4.17.21",
		"mobx": "^6.12.3",
		"mobx-react-lite": "^4.0.7",
		"nprogress": "^0.2.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-i18next": "^13.5.0",
		"react-redux": "^8.1.3",
		"react-router-dom": "^6.22.3",
		"react-transition-group": "^4.4.5",
		"redux": "^4.2.1",
		"redux-persist": "^6.0.0",
		"redux-promise": "^0.6.0",
		"redux-thunk": "^2.4.2",
		"screenfull": "^6.0.2",
		"three": "^0.154.0"
	},
	"devDependencies": {
		"@babel/plugin-proposal-decorators": "^7.24.1",
		"@commitlint/cli": "^19.2.1",
		"@commitlint/config-conventional": "^19.1.0",
		"@types/delaunator": "^5.0.2",
		"@types/express": "^4.17.21",
		"@types/geojson": "^7946.0.14",
		"@types/intro.js": "^5.1.5",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^18.19.29",
		"@types/nprogress": "^0.2.3",
		"@types/react": "^18.2.74",
		"@types/react-dom": "^18.2.23",
		"@types/react-redux": "^7.1.33",
		"@types/react-router-dom": "^5.3.3",
		"@types/redux-promise": "^0.5.32",
		"@types/three": "^0.152.1",
		"@typescript-eslint/eslint-plugin": "^7.5.0",
		"@typescript-eslint/parser": "^7.5.0",
		"@vitejs/plugin-react": "^4.2.1",
		"autoprefixer": "^10.4.19",
		"commitizen": "^4.3.0",
		"commitlint": "^19.2.1",
		"cz-git": "^1.9.1",
		"dotenv": "^16.4.5",
		"eslint": "^8.57.0",
		"eslint-config-love": "^44.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-n": "^15.7.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^9.0.11",
		"json-server": "^0.17.4",
		"less": "^4.2.0",
		"lint-staged": "^15.2.2",
		"mockjs": "^1.1.0",
		"nodemon": "^2.0.22",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"qs": "^6.12.0",
		"rollup-plugin-visualizer": "^5.12.0",
		"sass": "^1.72.0",
		"standard-version": "^9.5.0",
		"stylelint": "^15.9.0",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-recess-order": "^4.2.0",
		"stylelint-config-standard": "^33.0.0",
		"stylelint-config-standard-scss": "^9.0.0",
		"stylelint-less": "^1.0.6",
		"stylelint-order": "^6.0.3",
		"stylelint-prettier": "^5.0.0",
		"stylus": "^0.59.0",
		"ts-node": "^10.9.2",
		"typescript": "^4.9.5",
		"vite": "^4.5.3",
		"vite-plugin-babel": "^1.2.0",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-html": "^3.2.2",
		"vite-plugin-svg-icons": "^2.0.1"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-git"
		}
	},
	"lint-staged": {
		"**/*.{js,jsx,tsx,ts}": [
			"pnpm run lint:eslint",
			"git add ."
		],
		"**/*.{scss}": [
			"npm run lint:stylelint",
			"git add ."
		]
	},
	"packageManager": "pnpm@8.15.6"
}
